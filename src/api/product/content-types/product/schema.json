{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "t-shirt",
        "mug",
        "hoodie",
        "stickers",
        "shirt",
        "toy",
        "kids"
      ],
      "default": "t-shirt",
      "required": true
    },
    "size": {
      "type": "string",
      "maxLength": 70
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "red",
        "blue",
        "green",
        "yellow",
        "white",
        "orange",
        "black"
      ],
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "availableQty": {
      "type": "integer",
      "required": true
    },
    "brand": {
      "type": "string"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "orders": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::order.order",
      "inversedBy": "products"
    }
  }
}
